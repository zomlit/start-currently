{
  "name": "vscode-cursor-composer",
  "displayName": "Cursor Composer Integration",
  "description": "Handles cursor:// and vscode:// URIs to open errors in Cursor's composer",
  "version": "0.1.0",
  "publisher": "lst",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onUri",
    "onCommand:cursor-composer.openError"
  ],
  "contributes": {
    "commands": [
      {
        "command": "cursor-composer.openError",
        "title": "Open Error in Cursor Composer"
      }
    ],
    "uriHandlers": [
      {
        "schemes": [
          "cursor",
          "vscode-cursor-composer"
        ]
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "./node_modules/.bin/tsc -p ./",
    "watch": "./node_modules/.bin/tsc -watch -p ./",
    "package": "vsce package",
    "install-local": "code --install-extension vscode-cursor-composer-0.1.0.vsix"
  },
  "devDependencies": {
    "@types/node": "^16.18.121",
    "@types/vscode": "^1.93.0",
    "typescript": "^4.9.5",
    "vsce": "^2.15.0"
  }
}
